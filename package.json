{
  "name": "vnr-mindmap",
  "version": "1.0.0",
  "description": "Interactive mindmap for Vietnamese Communist Party History",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/intro.html",
    "start": "live-server --port=8080 --open=/index.html",
    "build": "echo 'Building static files...' && npm run minify",
    "minify": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/styles.min.css styles/*.css",
    "minify-js": "uglifyjs js/*.js -o dist/app.min.js",
    "serve": "http-server -p 3000 -o",
    "test": "echo 'No tests specified'",
    "lint": "eslint js/*.js",
    "format": "prettier --write *.html js/*.js styles/*.css"
  },
  "keywords": [
    "mindmap",
    "vietnamese-history",
    "communist-party",
    "interactive",
    "d3js",
    "education"
  ],
  "author": "VNR Mindmap Team",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "http-server": "^14.1.1",
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vnr-team/mindmap.git"
  },
  "bugs": {
    "url": "https://github.com/vnr-team/mindmap/issues"
  },
  "homepage": "https://vnr-team.github.io/mindmap"
}
