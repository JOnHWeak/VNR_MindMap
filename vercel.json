{
  "version": 2,
  "builds": [
    {
      "src": "js/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/js/server.js"
    },
    {
      "src": "/main",
      "dest": "/main.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
